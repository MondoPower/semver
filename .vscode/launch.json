    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\lib\\main.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "INPUT_SOLUTION": "${env:USERPROFILE}/source/repos/bitbucket/MATH/Math/src/Math.sln",
                "INPUT_ISDEFAULTBRANCH": "false",
                "INPUT_VERSIONSUFFIX": "personal",
                "INPUT_SOURCE": "http://artifacts.geomatic.com.au/nuget/nuget"
            }
        }
    ]
}